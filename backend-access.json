{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3BucketListAndLocation",
      "Effect": "Allow",
      "Action": [
        "s3:GetBucketLocation",
        "s3:ListBucket"
      ],
      "Resource": "arn:aws:s3:::tfstate-brant-202509221842"
    },
    {
      "Sid": "S3BackendObjectRW",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:AbortMultipartUpload"
      ],
      "Resource": "arn:aws:s3:::tfstate-brant-202509221842/*"
    },
    {
      "Sid": "DynamoDBStateLockTable",
      "Effect": "Allow",
      "Action": [
        "dynamodb:DescribeTable",
        "dynamodb:GetItem",
        "dynamodb:PutItem",
        "dynamodb:UpdateItem",
        "dynamodb:DeleteItem"
      ],
      "Resource": "arn:aws:dynamodb:us-east-2:803767876973:table/tf-locks"
    },
    {
      "Sid": "StsCallerIdentityForDebug",
      "Effect": "Allow",
      "Action": [
        "sts:GetCallerIdentity"
      ],
      "Resource": "*"
    }
  ]
}
